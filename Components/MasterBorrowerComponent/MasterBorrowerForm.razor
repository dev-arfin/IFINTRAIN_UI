<!-- #region Form -->
<TemplateForm Submit=@OnSubmit>
  <RadzenStack Gap="16">
    <!-- #region Toolbar -->
    <RadzenRow Gap="8">
       <RoleAccess Code="RC0003837">
          <Button ButtonType="ButtonType.Submit" ButtonStyle="ButtonStyle.Info" Text="Save" Icon="save" />
        </RoleAccess>
      <Button ButtonStyle="ButtonStyle.Danger" Text="Back" Icon="arrow_back" Click="@Back" />
      <Locker />
    </RadzenRow>
    <!-- #endregion -->

    <RadzenStack Gap="8">
      <RadzenRow Gap="32">
        <!-- #region Code -->
        <FormFieldTextBox Label="Code" Name="Code" Value="@(row["Code"]?.GetValue<string>())" Required="true"/>
        <!-- #endregion -->
        <!-- #region KTP NO -->
        <FormFieldTextBox Label="KTP No" Name="IDNo" Value="@(row["IDNo"]?.GetValue<string>())" Required="true"/>
        <!-- #endregion -->
        <!-- #region NAME -->
        <FormFieldTextBox Label="Name" Name="Name" Value="@(row["Name"]?.GetValue<string>())" Required="true"/>
        <!-- #endregion -->
      </RadzenRow>

      <RadzenRow Gap="32">
        <!-- #region Address -->
        <FormFieldTextBox Label="Address" Name="Address" Value="@(row["Address"]?.GetValue<string>())" Required="true"/>
        <!-- #endregion -->
      </RadzenRow>
    </RadzenStack>
  </RadzenStack>
</TemplateForm>
<!-- #endregion -->