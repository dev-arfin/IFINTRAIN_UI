@page "/borrowtransaction/transactionticket/{ID}/sellingdocument/add"
@page "/borrowtransaction/transactionticket/{ID}/sellingdocument/{DocID}"
@using IFinancing360_TRAINING_UI.Components.TransactionTicketSellComponent
@using IFinancing360_TRAINING_UI.Components.PanelMenuComponent
<RoleAccess Code="RA0001958">
<Title Text="Transaction Ticket Detail" />

<PanelMenuTicket UserID=@ID>
    <PageContainer>

    <TransactionTicketSellLabel ID=@ID ParentMenuURL=@ParentMenuURL />

    <Divider />
    
    <TransactionTicketSellDocForm 
        ID="@DocID"
        TransactionID="@ID"
        ParentMenuURL="@ParentMenuURL" />

    </PageContainer>
</PanelMenuTicket>
</RoleAccess>

@code {
    [Parameter] public string? ID { get; set; }
    [Parameter] public string? DocID { get; set; }
    [Parameter] public string? ParentMenuURL { get; set; }
}
